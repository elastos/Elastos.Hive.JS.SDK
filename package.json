{
  "name": "@elastosfoundation/hive-js-sdk",
  "version": "2.3.4",
  "description": "Elastos Hive JS SDK",
  "exports": {
    "node": "./dist/hive.js",
    "browser": {
      "import": "./dist/es/hive.browser.js"
    }
  },
  "main": "./dist/hive.js",
  "module": "./dist/es/hive.js",
  "browser": {
    "./dist/hive.js": "./dist/hive.browser.js",
    "./dist/es/hive.js": "./dist/es/hive.browser.js"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist/ dist.esm/ typings/ node_modules/",
    "tsc:build": "rollup -c",
    "types": "tsc --emitDeclarationOnly --outDir typings/ --project tsconfig.types.json",
    "tsc:w": "tsc -w",
    "test": "jest",
    "docs": "rm -Rf docs/ && npx typedoc",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "typings": "typings/",
  "dependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/fs-extra": "^9.0.13",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "axios": "^0.24.0",
    "browserfs": "^1.4.3",
    "dayjs": "^1.10.4",
    "jsrsasign": "^10.5.0",
    "jsrsasign-util": "^1.0.2",
    "postcss": "^8.4.4",
    "pretend": "^3.1.0",
    "rollup-plugin-file-content-replace": "^1.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "tslib": "^2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastos/Elastos.Hive.JS.SDK.git"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.13.0",
    "@elastosfoundation/did-js-sdk": "^2.2.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/bn.js": "^5.1.0",
    "@types/crypto-js": "^4.0.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.2",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.6",
    "path-browserify": "^1.0.1",
    "rollup": "^2.60.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^9.1.1",
    "typedoc": "^0.22.13",
    "typescript": "^4.5.2"
  },
  "files": [
    "dist",
    "typings"
  ],
  "include": [
    "typings",
    "dist"
